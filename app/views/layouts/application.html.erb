<!DOCTYPE html>
<html>
<head>
  <title>School Marks</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href="http://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.1/normalize.min.css" rel="stylesheet"></link>
  <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet"></link>
  <%= csrf_meta_tags %>
</head>

<body>
  <div class="container">
  <header>
    <div class="wrapper clearfix">
      <div id="logo">
        <%= link_to "%", root_path %>
      </div>
      <nav>
        <%= link_to "STUDENTS", students_path %>
        <%= link_to "SUBJECTS", subjects_path %>
        <%= link_to "TERMS", terms_path %>
        <%= link_to "RESULTS", results_path %>
        <%= link_to "REPORTS", reports_path %>
      </nav>
    </div>
  </header>
  <div class="wrapper">
    <%= yield %>
  </div>
  </div>
</body>
