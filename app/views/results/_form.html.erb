<%= simple_form_for(@result) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :classmark %>
    <%= f.input :testmark %>
    <%= f.association :student %>
    <%= f.association :subject %>
    <%= f.association :term %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
